                        18     init:
0018 75A800             19             MOV IE, #0h                     ; Wyłączenie wszystkich przerwań
001B C28C               20             CLR TR0                         ; Zatrzymanie timera T0
001D C28D               21             CLR TF0                         ; Wyzerowanie flagi przepełnienia
001F 758901             22             MOV TMOD, #01h                  ; Inicjalizacja timera T0
0022 120011             23             lcall reload_T0                 ; Ustawienie wartości timera
0025 75B800             24             MOV IP, #0h                     ; Ustawienie priorytetu przerwań na niski
0028 75A882             25             MOV IE, #82h                    ; Odblokuj przerwania timera T0
002B D28C               26             SETB TR0                        ; Uruchomienie odliczania
                        27
002D 12004A             28             lcall set_zero                  ; Ustawienie "zera" na wyświetlaczu
0030 80FE               29     main:   sjmp main                       ; Nieskończona pętla (czeka na przerwanie)
                        30
                        31
                        32     update_LED:
0032 A890               33             MOV R0, P1                      ; Załadowanie wartości portu wejściowego do R0
0034 58                 34             ANL A, R0                       ; Sprawdź, czy były zmiany na bitach portu P1
0035 B4FF01             35             CJNE A, #11111111b, update      ; Jeżeli były na jakimkolwiek, to zmień znak
0038 32                 36             RETI                            ; Jeżeli nic się nie zmieniło, nie zmieniaj znaku
                        37
                        38     update:                         ; Wyświetla konkretny znak na podstawie danych wejściowych
0039 E590               39             MOV A, P1               ; Zapisz aktualną wartość portu wejściowego do akumulatora
003B C0D0               40             PUSH PSW                ; Zapisz wartości znaczników na stos
003D C0E0               41             PUSH ACC                ; Zapisz wartość akumulatora na stos
                        42
003F 12004E             43             lcall find_corr_char    ; Załaduj do A sekwencję bitów znaku dla danego wejścia
0042 F8                 44             MOV R0, A               ; | Przepisz sekwencję bitów tego znaku z akumulatora
0043 88B0               45             MOV P3, R0              ; | do portu wyświetlacza P3, korzystając z R0
                        46
0045 D0E0               47             POP ACC                 ; Odzyskaj wartość akumulatora ze stosu
0047 D0D0               48             POP PSW                 ; Odzyskaj wartości znaczników ze stosu
0049 32                 49             RETI
                        50
                        51     set_zero:
004A 75B0C0             52             MOV P3, #11000000b              ; Wyświetl znak "0" na wyświetlaczu
004D 22                 53             RET
                        54
                        55
                        56     find_corr_char:
004E E590               57             MOV A, P1
0050 120055             58             call set_corr_char_DPTR_index   ; Ustaw prawidłowy indeks wskaźnika danych
0053 93                 59             MOVC A,@A+DPTR                  ; Pobierz sekwencję bitów szukanego znaku do AC
0054 22                 60             RET
                        61
                        62     set_corr_char_DPTR_index:
0055 F4                 63             CPL A   ; Odwróć bity A, bo segmenty wyświetlacza są zapalone dla stanów niskich
0056 903000             64             MOV DPTR, #3000h                ; Ustaw indeks początkowy
0059 B40002             65             CJNE A, #00h, increment_dptr    ; Wykonaj A iteracji, ustaw DPTR do wielkości A
                        66     DPTR_is_set:
005C E4                 67             CLR A   ; Rejestr DPTR ustawiony, zresetuj akumulator do wartości zerowej
005D 22                 68             RET
                        69
                        70     increment_dptr:                         ; Iteracyjnie zwiększaj DPTR aż nie będzie równy A
005E 7D19               71             MOV R5, #19h                    ; Wartość R5 mówi ile zostało do końca zakresu
                        72     loop:                                   ; Główna pętla ustalająca DPTR
0060 A3                 73             INC DPTR                        ; Zwiększ DPTR o 1
0061 1D                 74             DEC R5                          ; Zmniejsz R5 o 1
0062 BD0003             75             CJNE R5, #0h, cont              ; Jeżeli zakres nie został przepełniony, kontynuuj
0065 02005C             76             LJMP DPTR_is_set                ; Zakres przepełniony, więc kończymy pracę.
0068 D5E0F5             77     cont:   DJNZ A, loop                    ; Dekrementuj A aż nie będzie równe 0
006B 02005C             78             LJMP DPTR_is_set                ; Zakres nieprzepełniony, DPTR ustalone. Kończymy.
